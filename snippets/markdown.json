{
  "TOML Frontmatter": {
    "prefix": ["frontmatter", "header", "+++"],
    "body": [
      "+++",
      "title = \"${1:title}\"",
      "date = ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}T${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}${CURRENT_TIMEZONE_OFFSET}",
      "[taxonomies]",
      "tags = [\"${2:tag}\"]",
      "+++",
      "",
      ""
    ],
    "description": "Insert TOML frontmatter to be used in static site generators"
  },
  "More": {
    "prefix": ["more"],
    "body": ["<!--more-->", "", ""],
    "description": "Insert more delimiter to separate the content"
  }
}
